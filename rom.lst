3B3B: EE40    	        xor     %01000000   ; complement bit 6 (SIGN bit)
3B3D: 37      	        scf                 ; ensure a Non-Zero result
3B3E: 8F      	        adc     A,A         ; restore CARRY, complemented SIGN
              	                            ; ZERO flag = 0 for sure
